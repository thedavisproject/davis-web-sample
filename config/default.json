{
  "bunyan": {
    "core": {
      "name": "core",
      "level": "info",
      "streams": [{
        "type": "rotating-file",
        "path": "./log/core.log",
        "period": "1d",
        "count": 30,
        "level": "info"
      }]
    }
  },
  "storage": {
    "knex": {
      "client": "postgresql",
      "connection": {
        "host": "",
        "database": "",
        "user": "",
        "password": ""
      }
    },
    "transaction-timeout": 120000,
    "schema": "master",
    "targets": ["web"]
  },
  "jobQueue": {
    "name": "Davis-Queue",
    "config": {
      "redis": {
        "port": 6379,
        "host": "",
        "password": ""
      },
      "prefix": "davis-job-queue",
      "settings": {
        "lockDuration": 30000,
        "stalledInterval": 30000,
        "maxStalledCount": 1,
        "guardInterval": 5000,
        "retryPorcessDelay": 5000
      }
    }
  },
  "upload": {
    "path": "upload/"
  },
  "crypto": {
    "encryptionKey": "",
    "validationKey": ""
  }
}
