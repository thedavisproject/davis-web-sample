{
  "bunyan": {
    "core": {
      "name": "core",
      "level": "info",
      "streams": [{
        "type": "rotating-file",
        "path": "./log/core.log",
        "period": "1d",
        "count": 30,
        "level": "info"
      }]
    }
  },
  "storage": {
    "knex": {
      "client": "postgresql",
      "connection": {
        "host": "",
        "database": "",
        "user": "",
        "password": ""
      }
    },
    "transaction-timeout": 120000,
    "schema": "master",
    "targets": ["web"]
  },
  "upload": {
    "path": "upload/"
  }
}
